<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="673.0" prefWidth="960.0" style="-fx-background-color: linear-gradient(to top,#98A8F8, #BCCEF8, #FFE3E1);" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oodj.assignment.oopskylinecarrentalsystem.controller.customer.EditPasswordController">
    <Pane layoutX="140.0" layoutY="75.0" prefHeight="523.0" prefWidth="668.0" style="-fx-background-color: #EFEFEF; -fx-border-radius: 20; -fx-background-radius: 20; -fx-border-color: #000000;">
        <Pane layoutX="77.0" layoutY="90.0" prefHeight="387.0" prefWidth="542.0" style="-fx-background-color: #ffffff; -fx-border-color: #000000; -fx-background-radius: 20; -fx-border-radius: 20;">
            <Text fill="#25459e" layoutX="95.0" layoutY="41.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Current Password: " wrappingWidth="180.0">
                <font>
                    <Font name="Georgia Bold" size="15.0" />
                </font>
            </Text>
            <Button fx:id="changePasswordButton" layoutX="94.0" layoutY="315.0" mnemonicParsing="false" onAction="#onChangePasswordButtonClick" prefHeight="31.0" prefWidth="374.0" style="-fx-background-color: #DFF6FF; -fx-background-radius: 20; -fx-border-radius: 20; -fx-border-color: #000000;" text="Change Password">
                <font>
                    <Font name="Georgia Bold" size="15.0" />
                </font>
            </Button>
            <PasswordField fx:id="currentPasswordPasswordField" layoutX="95.0" layoutY="49.0" onKeyPressed="#onChangePasswordEnterKeyPressed" prefHeight="27.0" prefWidth="374.0" promptText="Enter Current Password" style="-fx-border-color: #000000;" />
            <Text fill="#25459e" layoutX="95.0" layoutY="118.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New Password:" wrappingWidth="180.0">
                <font>
                    <Font name="Georgia Bold" size="15.0" />
                </font>
            </Text>
            <PasswordField fx:id="newPasswordPasswordField" layoutX="95.0" layoutY="126.0" onKeyPressed="#onChangePasswordEnterKeyPressed" prefHeight="27.0" prefWidth="374.0" promptText="Enter New Password" style="-fx-border-color: #000000;" />
            <Text fill="#25459e" layoutX="94.0" layoutY="239.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Comfirm Again Your New Password:" wrappingWidth="318.5">
                <font>
                    <Font name="Georgia Bold" size="15.0" />
                </font>
            </Text>
            <PasswordField fx:id="confirmPasswordPasswordField" layoutX="94.0" layoutY="248.0" onKeyPressed="#onChangePasswordEnterKeyPressed" prefHeight="27.0" prefWidth="374.0" promptText="Enter New Password" style="-fx-border-color: #000000;" />
            <Label fx:id="currentPasswordWarningLabel" layoutX="95.0" layoutY="76.0" prefHeight="27.0" prefWidth="374.0" textFill="RED" wrapText="true">
                <font>
                    <Font name="Georgia" size="11.0" />
                </font>
            </Label>
            <Label fx:id="newPasswordWarningLabel" layoutX="94.0" layoutY="153.0" prefHeight="73.0" prefWidth="374.0" textFill="RED" wrapText="true">
                <font>
                    <Font name="Georgia" size="11.0" />
                </font>
            </Label>
            <Label fx:id="confirmPasswordWarningLabel" layoutX="95.0" layoutY="275.0" prefHeight="27.0" prefWidth="374.0" textFill="RED" wrapText="true">
                <font>
                    <Font name="Georgia" size="11.0" />
                </font>
            </Label>
        </Pane>
        <Text fill="#25459e" layoutX="189.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Change Password" wrappingWidth="318.5">
            <font>
                <Font name="Georgia Bold" size="34.0" />
            </font>
        </Text>
        <Button fx:id="homeButton" layoutX="562.0" layoutY="17.0" mnemonicParsing="false" onAction="#onHomeButtonClick" prefHeight="23.0" prefWidth="82.0" text="HOME">
            <font>
                <Font name="Georgia" size="12.0" />
            </font>
        </Button>
    </Pane>
    <Line endX="100.0" startX="-100.0" />
</AnchorPane>
