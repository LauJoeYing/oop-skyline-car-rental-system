<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="540.0" prefWidth="960.0" style="-fx-background-color: linear-gradient(to top,#98A8F8, #BCCEF8, #FFE3E1);" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oodj.assignment.oopskylinecarrentalsystem.Controller1">
    <Pane layoutX="30.0" layoutY="37.0" prefHeight="483.0" prefWidth="900.0" style="-fx-background-color: #EFEFEF; -fx-border-radius: 20; -fx-background-radius: 20; -fx-border-color: #000000;">
        <children>
          <Text fill="#153462" layoutX="39.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Manage Car Menu">
              <font>
                  <Font name="Georgia Bold" size="39.0" />
              </font>
          </Text>
         <TextField fx:id="searchInput" layoutX="39.0" layoutY="93.0" onInputMethodTextChanged="#listen" prefHeight="25.0" prefWidth="298.0" promptText="Search" style="-fx-background-radius: 20; -fx-border-color: #000000; -fx-border-radius: 20;">
            <font>
               <Font name="Georgia" size="12.0" />
            </font>
         </TextField>
         <TableView fx:id="carMenuTable" layoutX="40.0" layoutY="143.0" prefHeight="313.0" prefWidth="820.0" style="-fx-border-color: #000000; -fx-background-color: #FCFFE7;">
           <columns>
             <TableColumn fx:id="carIDCol" editable="false" prefWidth="111.0" text="Car ID" />
             <TableColumn fx:id="carBrandCol" editable="false" prefWidth="130.0" text="Car Brand" />
               <TableColumn fx:id="carModelCol" editable="false" prefWidth="119.0" text="Car Model" />
               <TableColumn fx:id="carTypeCol" editable="false" prefWidth="132.0" text="Car Type" />
               <TableColumn fx:id="carTransmissionCol" editable="false" prefWidth="145.0" text="Transmission" />
               <TableColumn fx:id="carPriceCol" editable="false" prefWidth="182.0" text="Rental Price (RM)" />
           </columns>
         </TableView>
         <Button layoutX="803.0" layoutY="16.0" mnemonicParsing="false" onAction="#toAdminMain" prefHeight="25.0" prefWidth="71.0" style="-fx-background-color: #ffffff; -fx-border-color: #000000;" text="HOME">
            <font>
               <Font name="Georgia Bold" size="12.0" />
            </font>
         </Button>
         <Button layoutX="360.0" layoutY="94.0" mnemonicParsing="false" onAction="#toAddCar" prefHeight="25.0" prefWidth="124.0" style="-fx-background-color: #DFF6FF; -fx-border-color: #000000; -fx-background-radius: 20; -fx-border-radius: 20;" text="Add New Car">
            <font>
               <Font name="Georgia Bold" size="12.0" />
            </font>
         </Button>
         <Button layoutX="498.0" layoutY="94.0" mnemonicParsing="false" onAction="#toModCar" prefHeight="25.0" prefWidth="124.0" style="-fx-background-color: #DFF6FF; -fx-border-color: #000000; -fx-background-radius: 20; -fx-border-radius: 20;" text="Modify Car">
            <font>
               <Font name="Georgia Bold" size="12.0" />
            </font>
         </Button>
        </children></Pane>
</AnchorPane>
